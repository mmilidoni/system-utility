execute pathogen#infect()
syntax on
filetype plugin indent on
" set number
" set mouse=a

let g:tagbar_usearrows = 1
nmap <F8> :TagbarToggle<CR>
nmap <leader>w :w!<cr>
set pastetoggle=<F2>

" Ignore case when searching
set ignorecase
" Highlight search results
set hlsearch

" 1 tab == 4 spaces
set shiftwidth=4
set tabstop=4

colorscheme peachpuff
